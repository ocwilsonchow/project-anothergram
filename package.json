    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "globals": {
      "$": true,
      "moment": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "comma-dangle": [
        2,
        "never"
      ],
      "max-len": 0,
      "radix": 0,
      "global-require": 0,
      "prefer-object-spread": 0,
      "object-curly-newline": 0,
      "no-param-reassign": [
        2,
        {
          "props": true,
          "ignorePropertyModificationsFor": [
            "draft",
            "req",
            "res"
          ]
        }
      ],
      "import/extensions": "off"
    }
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.6",
    "utf-8-validate": "^5.0.8"
  }
